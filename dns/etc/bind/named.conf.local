//include "/etc/bind/zones.rfc1918";
//forward lookup zone
zone "mitkar.io" IN {
	type master;
	file "/etc/bind/db.mitkar.io";
};

// reverse lookup zone
zone "0.168.192.in-addr.arpa" IN {
	type master;
	file "/etc/bind/db.0.168.192";
};
